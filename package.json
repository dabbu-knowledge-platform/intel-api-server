{
	"name": "intel-api-server",
	"version": "0.6.5",
	"description": "An implementation of the Dabbu Intel API that helps in extraction of and one pagers/summaries of all knowledge around a certain topic, person or place based on information/data from multiple providers",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"clean": "rm -r dist/ && mkdir dist/",
		"format": "xo --fix",
		"prebuild": "npm run test",
		"build": "npx pkg --output dist/intel-api-server . --targets node14.4.0-win-x64,node14.4.0-macos-x64,node14.4.0-linux-x64,node14.4.0-alpine-x64",
		"ci-build": "npm prune --production && npx pkg --output dist/intel-api-server . --targets node14.4.0-win-x64,node14.4.0-macos-x64,node14.4.0-linux-x64,node14.4.0-alpine-x64",
		"test": "xo"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dabbu-knowledge-platform/intel-api-server.git"
	},
	"author": "gamemaker1",
	"license": "GPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/dabbu-knowledge-platform/intel-api-server/issues"
	},
	"homepage": "https://github.com/dabbu-knowledge-platform/intel-api-server#readme",
	"dependencies": {
		"country-in-text-detector": "^1.0.10",
		"express": "^4.17.1",
		"fs-extra": "^9.1.0",
		"lda": "0.2.0",
		"multer": "^1.4.2",
		"office-text-extractor": "^1.3.1"
	},
	"devDependencies": {
		"pkg": "^4.5.1",
		"xo": "^0.38.2"
	},
	"bin": "src/index.js",
	"pkg": {
		"scripts": [
			"src/**/*"
		],
		"assets": [
			"node_modules/**/*"
		]
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"trailingComma": "none",
		"singleQuote": true,
		"bracketSpacing": true,
		"jsxBracketSameLine": false
	},
	"xo": {
		"semicolon": false,
		"prettier": true,
		"rules": {
			"complexity": 0,
			"no-unused-vars": 0,
			"max-params": 0,
			"max-depth": 0,
			"object-curly-spacing": 0,
			"unicorn/no-nested-ternary": 0
		},
		"ignores": [
			"node_modules/",
			"creds/",
			"dist/",
			"_dabbu/",
			".git/",
			".heroku/"
		]
	},
	"engines": {
		"node": "14.16.0",
		"npm": "7.7.6"
	}
}